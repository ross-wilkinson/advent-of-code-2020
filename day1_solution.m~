%% Advent of code - Day 1

expenseReport = table2array(readtable('day1_input.txt'));

%% Part A
diff = 2020 - expenseReport;
C = intersect(expenseReport,diff);
sol1 = C(1) * C(2)

%% Part B
A = bsxfun(@plus,expenseReport,expenseReport');
diff = 2020-A;
C = intersect(expenseReport,diff);
sol = C(1) * C(2) * C(3)